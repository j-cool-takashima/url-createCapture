{
    "rewrites": [
        {
            "source": "/",
            "destination": "/public/index.html"
        },
        {
            "source": "/((?!api).*)",
            "destination": "/public/$1"
        }
    ],
    "functions": {
        "api/capture.js": {
            "memory": 3008,
            "maxDuration": 30
        }
    }
}